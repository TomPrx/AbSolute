{
  "problem_set" : "benchmark/data/rcpsp/patterson.rcp/",
  "problem_kind" : <Patterson>,
  "solvers": [
    <AbSolute: {
      "domains": [
        { "name": "Octagon", "strategies": ["MSLF_simple"] },
        { "name": "Box", "strategies": ["First_fail_LB"] },
      ]
    }>,
    <MiniZinc: {
      "solvers": ["gecode", "chuffed"],
      "models": ["benchmark/data/minizinc/rcpsp-cumulative.mzn"],
      "strategies": ["int_search(s, smallest, indomain_min, complete)"]
    }>,
    <FlatMiniZinc: {
      "solvers": ["gecode", "chuffed"],
      "strategies": ["int_search(s, smallest, indomain_min, complete)"]
    }>
  ],
  "timeout" : 60,
  "print_solutions": false,
  "csv" : {
    "human" : true,
    "fields" : [<ProblemName>, <Time : <Sec>>, <Optimum>]
  }
}
